"Can you guess my name? You have three tries!">>printLine[];
let mut guesses = 0;
while guesses < 3 loop {
    let guess = readLine[];
    if guess == "Rumpelstiltskin" then {
        "You guesed my name!">>printLine[];
        return;
    } else {
        ("My name isn't " + guess)>>printLine[];
    };
    mut guesses = guesses + 1;
};
"You're out of guesses!">>printLine[];
